<%= form.select field.method_name, field.enum, { include_blank: true }.reverse_merge({ selected: field.form_value }),
                field.html_attributes.reverse_merge({ data: { enumeration: true }, placeholder: t('admin.misc.search') }) %>

<script type="text/javascript">
  $(document).ready(function () {
    $('#setup_stored_procedure_language').on('change', function (e) {
      var value = $(this).val(),
          help = $('#' + value + '-HELP');

      $('#setup_stored_procedure_code_field').prop('title', help.length ? help.html() : '');
    });

    $('#setup_stored_procedure_language').change();
  });
</script>

